<h1> Login here! </h1>

<% if flash[:notice] %>
<h2><%= flash[:notice] %></h2>
<% end %>

<%= form_tag("/sessions") do%>
    
    <%= label_tag "username" %>
    <%= text_field_tag "username" %>
    
    <%= label_tag "email" %>
    <%= text_field_tag "email" %>

    <%= submit_tag "Login" %>

<% end %>
<br><br>